    "def fact(n):\n",
    "    if n == 1 or n == 0:\n",
    "        return 1\n",
    "    else: \n",
    "        return n * fact(n-1)\n",
    "x = int(input(\"Enter the number\"))\n",
    "print(fact(x))      "
